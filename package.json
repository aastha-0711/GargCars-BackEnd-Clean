  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "mongoose": "^6.13.8",
    "nodemon": "^3.1.10",
    "stripe": "^8.222.0",
    "uuid": "^8.3.2"
  }
